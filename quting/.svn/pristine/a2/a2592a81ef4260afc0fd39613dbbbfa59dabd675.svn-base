package com.example.quting.entity.category;

import java.io.Serializable;

public class CategoryBaseEntity implements Serializable{

	private static final long serialVersionUID = 1L;
	
	/**
	 * {"created_at":"2013-06-20T10:43:48Z","id":1,
	 * "name":"热门小说","updated_at":"2013-06-20T10:43:48Z"
	 * {"categories":[{"created_at":"2013-06-20T05:59:55Z",
	 * "id":1,"name":"111","updated_at":"2013-06-20T05:59:55Z"}]}
	 * */
	private String created_at;
	private int id;
	private String name;
	private String updated_at;
	private String remark;
	public String getCreated_at() {
		return created_at;
	}
	public void setCreated_at(String created_at) {
		this.created_at = created_at;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getUpdated_at() {
		return updated_at;
	}
	public void setUpdated_at(String updated_at) {
		this.updated_at = updated_at;
	}
	public String getRemark() {
		return remark;
	}
	public void setRemark(String remark) {
		this.remark = remark;
	}
	
}
